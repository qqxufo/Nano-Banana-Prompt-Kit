{
  "manifest_version": 3,
  "name": "Nano Banana Prompt Kit",
  "version": "1.1.0",
  "description": "65+ curated AI prompts for Nano Banana. Quick copy and paste to create stunning images with popular styles.",
  "author": "NanaVis Team",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Nano Banana Prompts"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["sidePanel"],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["prompts.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
